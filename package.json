    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fork-ts-checker-webpack-plugin": "^0.2.8",
    "fs-extra": "7.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "24.1.0",
    "js-cookie": "^2.2.0",
    "lodash-es": "^4.17.11",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.2",
    "raf": "3.4.1",
